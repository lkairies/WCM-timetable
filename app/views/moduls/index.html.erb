<h1>Modul√ºbersicht</h1>
<table class="table table-hover">
 <tr>
  <th>Titel</th>
  <th>Nummer</th>
  <th>form</th>
  <th>credits</th>
  <th>lehrveranstaltungs</th>
 </tr>

 <% @moduls.each do |modul| %>
  <% if @modul_lvs.has_key?(modul.nummer) %>
    <tr>
     <td><%= link_to modul.titel, modul_path(modul.nummer) %></td>
     <td><%= modul.nummer %></td>
     <td><%= modul.form %></td>
     <td><%= modul.credits %></td>
     <td>
      <% @modul_lvs[modul.nummer].each do |lv_titel| %>
       <%= lv_titel %><br />
      <% end %>
     </td>
    </tr>
  <% end %>
 <% end %>
</table>
