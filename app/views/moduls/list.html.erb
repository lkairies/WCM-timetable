<h1>Moduls#list</h1>
<p>Find me in app/views/moduls/list.html.erb</p>
<table>
 <tr>
  <th>Titel</th>
  <th>Nummer</th>
  <th>Verantwortlich</th>
  <th>studiengang</th>
  <th>form</th>
  <th>credits</th>
  <th>semesterturnus</th>
  <th>verwendbarkeit</th>
  <th>lehrveranstaltungs</th>
 </tr>

 <% @moduls.each do |modul| %>
  <tr>
   <td><%= link_to modul.titel, modul_path(modul.nummer) %></td>
   <td><%= modul.nummer %></td>
   <td><%= modul.verantwortlich %></td>
   <td><%= modul.studiengang %></td>
   <td><%= modul.form %></td>
   <td><%= modul.credits %></td>
   <td><%= modul.semesterturnus %></td>
   <td><%= modul.verwendbarkeit %></td>
   <td>
    <% Lehrveranstaltung.where(modul_id: modul.nummer).each do |lv| %>
     <%= lv.titel %><br />
    <% end %>
   </td>
  </tr>
 <% end %>
</table>
